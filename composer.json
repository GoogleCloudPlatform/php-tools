{
    "name": "pratikid/php-tools",
    "description": "PHP tools for Google Cloud Platform",
    "keywords": ["test", "appengine", "gcp"],
    "homepage": "https://github.com/pratikid/php-tools",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Pratik Khanolkar",
            "email": "pratikid@gmail.com",
            "homepage": "https://www.google.com/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "symfony/browser-kit": "~2|~3",
        "symfony/console": "~4.2",
        "symfony/filesystem": "~2|~3",
        "symfony/process": "~4",
        "twig/twig": "~1.3|~2.0"
    },
    "bin": [
        "src/Utils/Flex/flex_exec",
        "scripts/install_test_deps.sh"
    ],
    "autoload": {
        "psr-4": {
            "Google\\Cloud\\TestUtils\\": "src/TestUtils/",
            "Google\\Cloud\\Utils\\": "src/Utils/"
        }
    },
    "require-dev": {
        "google/cloud-core": "^1.20",
        "google/gax": "^0.35.0",
        "paragonie/random_compat": ">=2"
    }
}
